{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "(tabs)/index.jss",
    "(tabs)/_layout.jss",
    "(tabs)/Voicenote.js",
    "components/AppleStyleSwipeableRow.js",
    "components/AppleStyleSwipeableRow.js",
    "components/Memolistitem.js",
    "expo-env.d.ts"
  ]
}
